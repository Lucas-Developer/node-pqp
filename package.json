{
  "name": "pqp",
  "version": "0.0.1",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es ",
    "build:commonjs": "babel src -d dist/commonjs ",
    "build:es": "cross-env BABEL_ENV=es babel src -d dist/es",
    "prebuild:commonjs": "rimraf dist/commonjs",
    "prebuild:es": "rimraf dist/es",
    "prerelease": "cross-env NODE_ENV=production npm run build",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "test": "mocha --require babel-core/register --reporter spec test/*-test.js"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "asn1.js": "^5.0.0",
    "bignum": "^0.12.5",
    "crypto-js": "^3.1.9-1",
    "node-bitarray": "^0.1.0",
    "numjs": "^0.15.1",
    "sprintf-js": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "mocha": "^4.0.1"
  }
}
